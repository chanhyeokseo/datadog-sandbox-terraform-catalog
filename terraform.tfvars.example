# ============================================
# Terraform Variables Example
# ============================================
# Copy this file to terraform.tfvars and update with your values
# cp terraform.tfvars.example terraform.tfvars

# External VPC Configuration
vpc_id            = "vpc-xxxxxxxxx"
public_subnet_id  = "subnet-xxxxxxxxx"
public_subnet2_id = "subnet-xxxxxxxxx"
private_subnet_id = "subnet-xxxxxxxxx"

# Personal project settings
project_name = "your-name"
project_env  = "test"

# EC2 configuration
# Replace with your AWS EC2 key pair name
ec2_key_name = "your-key-pair-name"

# Datadog configuration
# Get your API key from: https://app.datadoghq.com/organization-settings/api-keys
datadog_api_key = "your-datadog-api-key-here"